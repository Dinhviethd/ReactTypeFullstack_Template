    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.3",
    "@types/nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
